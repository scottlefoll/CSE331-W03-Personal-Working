        "/contacts/update/{_id}": {
            "post": {
                "description": "Update the contact with the specified id in Contacts.",
                "summary": "Update contact by id",
                "parameters": [
                    {
                        "name": "_id",
                        "in": "path",
                        "required": true,
                        "description": "The id of the contact to update",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "firstName": {
                                        "type": "string"
                                    },
                                    "lastName": {
                                        "type": "string"
                                    },
                                    "favoriteColor": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "birthday": {
                                        "type": "string"
                                    }
                                }
                            },
                            "examples": {
                                "0": {
                                    "value": "{\n    \"firstName\": \"Zoe\",\n    \"lastName\": \"Smithson\",\n    \"email\": \"zsmithson@gmail.com\",\n    \"favoriteColor\": \"blue\",\n    \"birthday\": \"1990-05-15T00:00:00.000Z\"\n}"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success!",
                        "content": {
                            "application/json":{
                                "schema": {
                                    "type": "string"
                                },
                                "examples": {}
                            }
                        }
                    }
                }
            }
        },
